{
    "version": 2,
    "functions": {
        "api/index.js": {
            "runtime": "nodejs20.x",
            "includeFiles": "api/**"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/((?!api/).*)",
            "destination": "/index.html"
        }
    ]
}